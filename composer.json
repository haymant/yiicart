{
    "name": "haymant/yiicart",
    "description": "yii module wrapping simpleCartJs and more.",
    "keywords": ["simpleCartJs", "yii"],
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Li, Zhao",
            "email": "dev@lizhao.net",
            "homepage": "http://lizhao.net/"
        }
    ],
    "require-dev": {
        "codeception/codeception": "@stable",
        "crisu83/yii-composer": ">=1.0.0",
        "yiisoft/yii": ">=1.1.13"
    },
    "scripts": {
        "post-install-cmd": [
            "Crisu83\\YiiComposer\\InstallHandler::setPermissions"
        ]
    },
    "extra": {
        "yii-install-writable": [
            "tests/_log"
        ]
    },
    "autoload": {
        "classmap": [
            "behaviors/",
            "components/",
            "form/",
            "helpers/",
            "widgets/"
        ]
    }
}
